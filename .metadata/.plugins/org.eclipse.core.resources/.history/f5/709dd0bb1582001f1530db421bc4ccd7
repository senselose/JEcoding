package Lv1_Programmers;

import java.util.ArrayList;
import java.util.Arrays;

public class prep {

	public static void main(String[] args) {
		
		int [] A = {1,3,6,4,1,2};
		
		ArrayList<Integer> arrList1 = new ArrayList<>();
		
		Arrays.sort(A);
		
		for(int i : A) {
			arrList1.add(i);
			System.out.println(i);
		}
		
		
	}

}
